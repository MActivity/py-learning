    "import random\n",
    "import math\n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "# define DataFrame\n",
    "random_walk_points = pd.DataFrame(columns=['x','y','distance'])\n",
    "dist_list = []\n",
    "\n",
    "# Create a function that maps out random walk\n",
    "def random_walk(num_blocks):    \n",
    "    global random_walk_points\n",
    "    global distance\n",
    "    coords=[(0,0,0)]\n",
    "    indices = [-1]\n",
    "    for i in range(num_blocks):\n",
    "        \n",
    "        # Choose direction\n",
    "        direction = random.choice(['n','s','w','e'])\n",
    "        \n",
    "        # Change coordinates\n",
    "        if direction == 'n':\n",
    "            coords.append((coords[i][0]+1, coords[i][1], math.hypot((coords[i][0]+1)-coords[0][0], coords[i][1]-coords[0][1])))\n",
    "        elif direction == 's':\n",
    "            coords.append((coords[i][0]-1, coords[i][1], math.hypot((coords[i][0]-1)-coords[0][0], coords[i][1]-coords[0][1])))\n",
    "        elif direction == 'w':\n",
    "            coords.append((coords[i][0], coords[i][1]-1, math.hypot(coords[i][0]-coords[0][0], (coords[i][1]-1)-coords[0][1])))\n",
    "        elif direction == 'e':\n",
    "            coords.append((coords[i][0], coords[i][1]+1, math.hypot(coords[i][0]-coords[0][0], (coords[i][1]+1)-coords[0][1])))\n",
    "            \n",
    "    # Create data frame of coordinates\n",
    "    random_walk_points = pd.DataFrame(coords,columns=['x','y','distance'])\n",
    "    dist_list.append(random_walk_points.iloc[-1][-1])\n",
    "    distance = pd.DataFrame(dist_list, columns=['distance'])\n",
    "    \n",
    "\n",
    "    \n",
    "    \n",
    "    # Plot Coordinates\n",
    "    plt.plot(random_walk_points['x'],random_walk_points['y'])\n",
    "    plt.show\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "\n",
    "# create a function that takes multiple walks \n",
    "\n",
    "def multiple_walks(num_walks, num_steps):\n",
    "    for i in range(num_walks):\n",
    "        random_walk(num_steps)\n",
    "    distance.describe()
    "    "
   ]
  },
